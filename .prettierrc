{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(next/(.*)$)|^(next$)",
    "<THIRD_PARTY_MODULES>",
    "^@/(?!.*.(css|scss|svg)$).*",
    "^./(?!.*.(css|scss|svg)$).*",
    "^../(?!.*.(css|scss|svg)$).*",
    ".svg$",
    "^(?!.*[.]css$)[./].*$",
    ".css$"
  ]
}
